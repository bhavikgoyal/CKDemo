@model IEnumerable<ChossonKallah.Models.BusinessReviewClass>





<script>
    $(document).ready(function () {
        var mydata = $('#RawDatas').parent('td').html();
        $('#RawDatas').parent().parent().parent().parent('table').replaceWith(mydata);

        $("#AddressBookToggleAll").change(function () {
            if ($(this).is(':checked')) {
                $("input.k-checkbox[type=checkbox]").each(function () {
                    $(this).attr('checked', true);
                });
            } else {
                $("input.k-checkbox[type=checkbox]").each(function () {
                    $(this).attr('checked', false);
                });
            }
        });
    });
</script>








<table class="table table-striped table-bordered bootstrap-datatable datatable dataTable">
    <tr>
        <th>
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Businessid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Review)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rating)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Isactive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Addedon)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Addedbyip)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Isdeleted)
        </th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.HiddenFor(model => item.Businessreviewid)
            </td>
            <td>
                @Html.DisplayFor(model => item.Businessid)
            </td>
            <td>
                @Html.DisplayFor(model => item.Name)
            </td>
            <td>
                @Html.DisplayFor(model => item.Email)
            </td>
            <td>
                @Html.DisplayFor(model => item.Review)
            </td>
            <td>
                @Html.DisplayFor(model => item.Rating)
            </td>
            <td>
                @Html.DisplayFor(model => item.Isactive)
            </td>
            <td>
                @Html.DisplayFor(model => item.Addedon)
            </td>
            <td>
                @Html.DisplayFor(model => item.Addedbyip)
            </td>
            <td>
                @Html.DisplayFor(model => item.Isdeleted)
            </td>
            <td>
                <a class="btn btn-info" style="padding:2px 7px; margin:2px;" href="/BusinessReview/Edit?Businessreviewid=@item.Businessreviewid "><i class="halflings-icon white edit"></i></a>
                <a class="btn btn-success" style="padding:2px 7px; margin:2px;" href="/BusinessReview/Details?Businessreviewid=@item.Businessreviewid "><i class="halflings-icon white zoom-in"></i></a>
                <a class="btn btn-danger" style="padding:2px 7px; margin:2px;" href="/BusinessReview/Delete?Businessreviewid=@item.Businessreviewid" onclick="return confirm('Are you sure you want to delete this record.'); "><i class="halflings-icon white trash"></i></a>
            </td>
        </tr>
    }
</table>







