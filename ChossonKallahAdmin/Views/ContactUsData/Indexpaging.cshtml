 @model IEnumerable<ChossonKallah.Models.ContactUsDataClass> 
 
 



<script>
		 $(document).ready(function () {
			 var mydata = $('#RawDatas').parent('td').html();
			 $('#RawDatas').parent().parent().parent().parent('table').replaceWith(mydata);
		 
			 $("#AddressBookToggleAll").change(function () {
				 if ($(this).is(':checked')) {
					 $("input.k-checkbox[type=checkbox]").each(function () {
						 $(this).attr('checked', true);
					 }); 
				 } else {
					 $("input.k-checkbox[type=checkbox]").each(function () {
						  $(this).attr('checked', false);
					 });
				 }
			 }); 
		 }); 
 </script>
 


 
  


 
		 <table class="table table-striped table-bordered bootstrap-datatable datatable dataTable" >
			 <tr>
				 <th>
				 </th>
				 <th >
					 @Html.DisplayNameFor(model => model.Name)
				 </th>
				 <th >
					 @Html.DisplayNameFor(model => model.Email)
				 </th>
				 <th >
					 @Html.DisplayNameFor(model => model.Messagesubject)
				 </th>
				 <th >
					 @Html.DisplayNameFor(model => model.Detailedmessage)
				 </th>
				 <th >
					 @Html.DisplayNameFor(model => model.Addeddatetime)
				 </th>
				 <th >
					 @Html.DisplayNameFor(model => model.Addedip)
				 </th>
				 <th>Action</th>
			 </tr>
		 @foreach (var item in Model) {
			 <tr>
				 <td>
					 @Html.HiddenFor(model => item.Contactusdataid)
				 </td>
				 <td >
					 @Html.DisplayFor(model => item.Name)
				 </td>
				 <td >
					 @Html.DisplayFor(model => item.Email)
				 </td>
				 <td >
					 @Html.DisplayFor(model => item.Messagesubject)
				 </td>
				 <td >
					 @Html.DisplayFor(model => item.Detailedmessage)
				 </td>
				 <td >
					 @Html.DisplayFor(model => item.Addeddatetime)
				 </td>
				 <td >
					 @Html.DisplayFor(model => item.Addedip)
				 </td>
				 <td><a class="btn btn-info" style="padding:2px 7px; margin:2px;" href="/ContactUsData/Edit?Contactusdataid=@item.Contactusdataid "><i class="halflings-icon white edit"></i></a>  
				 <a class="btn btn-success" style="padding:2px 7px; margin:2px;" href="/ContactUsData/Details?Contactusdataid=@item.Contactusdataid "><i class="halflings-icon white zoom-in"></i></a>  
				 <a class="btn btn-danger" style="padding:2px 7px; margin:2px;" href="/ContactUsData/Delete?Contactusdataid=@item.Contactusdataid" onclick="return confirm('Are you sure you want to delete this record.'); "><i class="halflings-icon white trash"></i></a> </td>
			 </tr>
		 }
		 </table>
		 
	 


 

   
